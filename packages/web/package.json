{
  "name": "@redwoodjs/web",
  "version": "0.0.0-experimental-streaming.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/web"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./serverContext": {
      "require": "./dist/components/serverContext.js",
      "import": "./dist/components/serverContext.mjs"
    },
    "./apollo": {
      "require": "./dist/apollo/index.js",
      "import": "./dist/apollo/index.mjs",
      "types": "./dist/apollo/index.d.ts"
    },
    "./graphql": {
      "require": "./dist/graphql.js",
      "import": "./dist/graphql.mjs"
    },
    "./DevFatalErrorPage": {
      "require": "./dist/components/DevFatalErrorPage.js",
      "import": "./dist/components/DevFatalErrorPage.mjs"
    }
  },
  "bin": {
    "build-storybook": "./dist/bins/build-storybook.js",
    "cross-env": "./dist/bins/cross-env.js",
    "msw": "./dist/bins/msw.js",
    "redwood": "./dist/bins/redwood.js",
    "rw": "./dist/bins/redwood.js",
    "rwfw": "./dist/bins/rwfw.js",
    "start-storybook": "./dist/bins/start-storybook.js",
    "tsc": "./dist/bins/tsc.js",
    "webpack": "./dist/bins/webpack.js"
  },
  "files": [
    "dist",
    "toast",
    "src/entry"
  ],
  "scripts": {
    "build": "yarn build:js",
    "build:js": "yarn tsup && yarn build:types",
    "build:types": "yarn tsc --emitDeclarationOnly",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build\"",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "jest src",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@apollo/client": "3.7.11",
    "@babel/runtime-corejs3": "7.21.0",
    "@redwoodjs/auth": "0.0.0-experimental-streaming.7",
    "core-js": "3.30.0",
    "graphql": "16.6.0",
    "graphql-esm": "16.5.0",
    "graphql-tag": "2.12.6",
    "react-helmet-async": "1.3.0",
    "react-hot-toast": "2.4.0",
    "stacktracey": "2.1.8",
    "ts-toolbelt": "9.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/prop-types": "15.7.5",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "5.14.5",
    "jest": "29.5.0",
    "jest-runner-tsd": "5.0.0",
    "nodemon": "2.0.22",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^6.7.0",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "gitHead": "867c1182a55d1506866ccd42db445bb89c46fd67"
}
